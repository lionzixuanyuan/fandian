<h1>Listing food_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Shop</th>
    <th>Enable</th>
    <th>Sort</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @food_types.each do |food_type| %>
  <tr>
    <td><%= food_type.name %></td>
    <td><%= food_type.shop_id %></td>
    <td><%= food_type.enable %></td>
    <td><%= food_type.sort_id %></td>
    <td><%= link_to 'Show', food_type %></td>
    <td><%= link_to 'Edit', edit_food_type_path(food_type) %></td>
    <td><%= link_to 'Destroy', food_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Food type', new_food_type_path %>
