<h1>Listing shop_hours</h1>

<table>
  <tr>
    <th>Shop</th>
    <th>Start time</th>
    <th>End time</th>
  </tr>

<% @shop_hours.each do |shop_hour| %>
  <tr>
    <td><%=h shop_hour.shop_id %></td>
    <td><%=h shop_hour.start_time %></td>
    <td><%=h shop_hour.end_time %></td>
    <td><%= link_to 'Show', shop_hour %></td>
    <td><%= link_to 'Edit', edit_shop_hour_path(shop_hour) %></td>
    <td><%= link_to 'Destroy', shop_hour, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New shop_hour', new_shop_hour_path %>