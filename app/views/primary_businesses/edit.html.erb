<h1>Editing primary_business</h1>

<% form_for(@primary_business) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :enbale %><br />
    <%= f.check_box :enbale %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @primary_business %> |
<%= link_to 'Back', primary_businesses_path %>