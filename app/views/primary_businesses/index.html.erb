<h1>Listing primary_businesses</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Enable</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @primary_businesses.each do |primary_business| %>
  <tr>
    <td><%= primary_business.name %></td>
    <td><%= primary_business.enable %></td>
    <td><%= link_to 'Show', primary_business %></td>
    <td><%= link_to 'Edit', edit_primary_business_path(primary_business) %></td>
    <td><%= link_to 'Destroy', primary_business, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

